{
  "schemaVersion": 1,
  "id": "itemattachments",
  "version": "${version}",
  "name": "Item Attachments",
  "description": "An API and screen utility for simple and immersive item attachments.\nInspired by Unturned and Crayfish's Gun Mod.",
  "authors": [
    "rbasamoyai"
  ],
  "contact": {
    "homepage": "https://github.com/Wagers-of-Industrial-Warfare/",
    "sources": "https://github.com/Wagers-of-Industrial-Warfare/ItemAttachments"
  },
  "license": "MIT License",
  "icon": "assets/itemattachments/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "rbasamoyai.itemattachments.fabric.ItemAttachmentsModFabric"
    ],
    "client": [
      "rbasamoyai.itemattachments.fabric.IAModClientFabric"
    ]
  },
  "mixins": [
    "itemattachments.mixins.json",
    "itemattachments-common.mixins.json"
  ],
  "depends": {
    "minecraft": "${minecraft_version}",
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}"
  }
}